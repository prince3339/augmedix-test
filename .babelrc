{
  "env": {
    "development": {
       "plugins": [
          [
            "styled-components", { "ssr": true, "displayName": true, "preprocess": false }
          ],
          [
            "module-resolver", {
              "root": ["./"],
              "alias": {
                "@": "./",
                "components": "./components"
              }
            }
          ]
        ],
        "presets": ["next/babel"]
    },
    "production": {
       "plugins": [
          [
            "styled-components", { "ssr": true, "displayName": true, "preprocess": false }
          ],
          [
            "module-resolver", {
              "root": ["./"],
              "alias": {
                "@": "./",
                "components": "./components"
              }
            }
          ]
        ],
        "presets": ["next/babel"]
      }
  },
  "plugins": [
    [
      "styled-components", { "ssr": true, "displayName": true, "preprocess": false }
    ],
    [
      "module-resolver", {
        "root": ["./"],
        "alias": {
          "@": "./",
          "components": "./components"
        }
      }
    ]
  ],
  "presets": ["@babel/preset-env"]
}